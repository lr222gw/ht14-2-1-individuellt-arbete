@model Weather.MVC.viewModels.ViewModel

@foreach (var item in Model.Locations)
{
    <div class="locationItem">
        <h3>@Html.ActionLink(item.Name, "Forecast", new { geonameID = item.GeoLocationID })</h3>
        <p>@Html.DisplayTextFor(model => item.Country)</p>
        
        
    </div>
}
